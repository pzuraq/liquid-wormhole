{
  "name": "liquid-wormhole",
  "version": "4.0.0",
  "description": "Wormholes with the power of Liquid Fire",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/pzuraq/liquid-wormhole",
  "license": "MIT",
  "author": "Christopher Garrett <cmg@pzuraq.co>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/jquery": "^2.0.0",
    "@ember/render-modifiers": "^3.0.0",
    "@embroider/util": "^1.13.4",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.3.0",
    "perf-primitives": "github:RobbieTheWagner/perf-primitives#a6a26f11497ca27be3763a88a5f20744e424756b"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^2.9.6",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "bootstrap-sass": "~3.4.3",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~4.12.3",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-markdown-compiler": "github:RobbieTheWagner/ember-cli-markdown-compiler#930f9605920b90cb0ddd447750394f68b4ef72dd",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.1.1",
    "ember-source": "~4.12.4",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-qunit": "^7.3.4",
    "liquid-fire": "^0.34.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.7.3",
    "qunit": "^2.24.3",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.11.0",
    "release-it-lerna-changelog": "^5.0.0",
    "sass": "^1.94.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-prettier": "^5.0.3",
    "webpack": "^5.103.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0",
    "liquid-fire": "^0.34.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "liquid-fire"
    ]
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "highlight.js"
    ]
  }
}
