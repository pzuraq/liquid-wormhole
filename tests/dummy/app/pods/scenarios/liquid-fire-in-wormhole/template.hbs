<button data-test-toggle-inner type="button" {{action "toggleContent"}}>
  Toggle Inner Content
</button>

<LiquidWormhole @class="blue-box">
  <div id="content-box">
    {{#liquid-if showingOther}}
      <span id="showing-other">Show Other</span>
    {{else}}
      <span id="not-showing-other">Don't Show Other</span>
    {{/liquid-if}}
  </div>
</LiquidWormhole>
